{
  "name": "rez",
  "version": "0.1.0",
  "description": "ウェブシーケンサの試み",
  "scripts": {
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "clean-dist": "npm run remove-dist && mkdir ./static",
    "remove-dist": "node_modules/.bin/rimraf ./static",
    "start": "npm-run-all --parallel test:watch server lint:watch",
    "server": "babel-node tools/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/tests.js \"./{,!(node_modules)/**/}*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/przhkv/rez.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/przhkv/rez/issues"
  },
  "homepage": "https://przhkv.github.io/rez",
  "dependencies": {
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.1",
    "redux": "3.6.0",
    "redux-saga": "0.14.2"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "css-loader": "0.26.1",
    "eslint": "3.13.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.9.0",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "immutable": "3.8.1",
    "mocha": "3.2.0",
    "node-sass": "4.3.0",
    "npm-run-all": "4.0.0",
    "redux-immutable-state-invariant": "1.2.4",
    "reselect": "2.5.4",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "tachyons-sass": "4.7.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0"
  }
}
