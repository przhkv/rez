{
  "name": "rez",
  "version": "0.1.0",
  "description": "ウェブシーケンサの試み",
  "scripts": {
    "build:html": "babel-node dev-tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node dev-tools/build.js",
    "clean-dist": "yarn run remove-dist && mkdir ./static",
    "remove-dist": "rimraf ./static",
    "start": "npm-run-all --parallel test:watch server lint:watch",
    "server": "babel-node dev-tools/server.js",
    "lint": "esw webpack.config.* src dev-tools",
    "lint:watch": "yarn run lint -- --watch",
    "test": "mocha --reporter progress dev-tools/tests.js \"./{,!(node_modules)/**/}*.spec.js\"",
    "test:watch": "yarn run test -- --watch",
    "upg": "yarn upgrade-interactive --exact",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/przhkv/rez.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/przhkv/rez/issues"
  },
  "homepage": "https://przhkv.github.io/rez",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-register": "6.26.0",
    "cheerio": "1.0.0-rc.2",
    "colors": "1.1.2",
    "express": "4.17.3",
    "history": "4.6.3",
    "immutable": "3.8.1",
    "path-to-regexp": "1.7.0",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-saga": "0.15.6",
    "reselect": "3.0.1",
    "styled-components": "2.1.2",
    "webpack": "^4.41.5",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.1.1",
    "css-loader": "^3.4.2",
    "eslint": "4.5.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.2.1",
    "eslint-watch": "3.1.2",
    "extract-text-webpack-plugin": "3.0.0",
    "husky": "0.14.3",
    "lint-staged": "4.0.3",
    "mocha": "3.5.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "tachyons-sass": "^4.9.5",
    "url-loader": "^3.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
